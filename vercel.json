{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/cron/full-sync",
      "schedule": "45 7,13 * * *"
    }
  ]
}
